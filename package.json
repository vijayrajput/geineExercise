{
  "name": "geineExercise",
  "version": "1.0.0",
  "description": "Demo Application for Geine Workshop",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/attachments": "^1.1.1",
    "@sap-cloud-sdk/http-client": "^3.15.0",
    "@sap-cloud-sdk/util": "^3.15.0",
    "@sap/cds": "^7",
    "@sap/cds-hana": "^2",
    "@sap/low-code-event-handler": "^1.2.4",
    "@sap/mdk-tools": "^1.3.1",
    "@sap/xsenv": ">=3.3.2 <3.3.3",
    "@sap/xssec": "^3.6.1",
    "cap-llm-plugin": "^1.3.2",
    "express": "^4",
    "hdb": "^0.19.8",
    "passport": "^0.7.0",
    "pdf-parse": "^1.1.1",
    "stream-to-blob": "^2.0.1"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/dev-cap-tools": "^1.31.0",
    "@sap/eslint-plugin-cds": "^3",
    "@sap/ux-specification": "^1.108.31",
    "langchain": "^0.2.5",
    "eslint": "^9"
  },
  "scripts": {
    "start": "cds-serve",
    "start-local": "npm run generate-entry-point && npm run cds-watch",
    "generate-entry-point": "dev-cap-tools gen-entrypoint",
    "cds-watch": "cds watch --open",
    "watch-geineExercise.manageKnowledgeDocuments": "cds watch --open geineExercise.manageKnowledgeDocuments/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "requires": {
      "[production]": {
        "db": "hana",
        "auth": "xsuaa"
      },
      "attachments": {
        "scan": false,
        "kind": "db"
      },
      "cap-llm-plugin": true,
      "destinations": true,
      "GENERATIVE_AI_HUB": {
        "CHAT_MODEL_DESTINATION_NAME": "AICoreAzureOpenAIDestination",
        "CHAT_MODEL_DEPLOYMENT_URL": "/inference/deployments/dc0322eee5f9a04d",
        "CHAT_MODEL_RESOURCE_GROUP": "genie",
        "CHAT_MODEL_API_VERSION": "2023-05-15",
        "EMBEDDING_MODEL_DESTINATION_NAME": "AICoreAzureOpenAIDestination",
        "EMBEDDING_MODEL_DEPLOYMENT_URL": "/inference/deployments/d130f4e857abdf1c",
        "EMBEDDING_MODEL_RESOURCE_GROUP": "genie",
        "EMBEDDING_MODEL_API_VERSION": "2023-05-15"
      },
      "AICoreAzureOpenAIDestination": {
        "kind": "rest",
        "credentials": {
          "destination": "GENERATIVE_AI_HUB",
          "requestTimeout": "300000"
        }
      },
      "app-service": {
        "impl": "@sap/low-code-event-handler"
      },
      "auth": {
        "[development]": {
          "kind": "dummy"
        },
        "[production]": {
          "kind": "xsuaa"
        }
      }
    },
    "features": {
      "fetch_csrf": true
    }
  },
  "lcap": true,
  "sapux": [
    "app/geineExercise.manageKnowledgeDocuments"
  ]
}
